#!/usr/bin/env node
'use strict';
const Web3 = require('web3');
const EthProbe = require('../index');

const web3 = new Web3();
web3.setProvider(new web3.providers.HttpProvider('http://localhost:8545'));

const ethProbe = new EthProbe(web3);

let promiseTxsFast = ethProbe.tx.toAddressFast('0x8bdfcef3d5ec77a77985d07925b05e5f9302b9a8', {fromOffset: 2000});
promiseTxsFast.then(txs => console.log(txs));

// let promiseTxsFull = ethProbe.tx.toAddressFullScan('0x8bdfcef3d5ec77a77985d07925b05e5f9302b9a8', {fromOffset: 200});
// promiseTxsFull.then(txs => console.log(txs));
